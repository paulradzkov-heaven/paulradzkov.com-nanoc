/*
Table Styles
Compability: All


Styleguide 3.1.1.
*/

table.chars {width:auto;margin:2em 0;margin:2rem 0;border-collapse:collapse;}
table.chars td, table.chars th {border:1px dotted #CCC;border-width:1px 0;padding:0.5em 2em;padding:0.5rem 2rem;}
table.chars th {border-width:0 0 1px;}
.char-demo {font-size:3em;font-size:3rem;font-family:serif;text-align:center;vertical-align:middle;width:125px;}
.char-demo.char-spaces {color:#999;font-size:1.5em;font-size:1.5rem !important;}

table.chars.layout-flow {display:block;padding-right:120px;}
table.chars.layout-flow thead {display:none;}
table.chars.layout-flow tr {display:block;float:left;width:119px;height:119px;border:1px solid #AAA;margin:-1px 0 0 -1px;background:#FFF;position:relative;}
table.chars.layout-flow tr.ch-section {width:239px;height:119px;background:#EEE;}
table.chars.layout-flow tr.ch-section th {display:table-cell;height:119px;border:none;padding:20px 20px;vertical-align:middle;}
table.chars.layout-flow tr td {border:none;position:absolute;}
table.chars.layout-flow tr td.char-demo {position:relative;z-index:1;display:table-cell;vertical-align:middle;width:118px;height:119px;background:#FFF;padding:0 10px;}
table.chars.layout-flow tr td.char-entity,
table.chars.layout-flow tr td.char-code,
table.chars.layout-flow tr td.char-descr {visibility:hidden;background:#FFF;border:1px solid #AAA;
	-webkit-transition: none !important;
	   -moz-transition: none !important;
			transition: none !important;
}
table.chars.layout-flow tr:hover td.char-entity,
table.chars.layout-flow tr:hover td.char-code,
table.chars.layout-flow tr:hover td.char-descr {visibility:visible;z-index:2;border:1px solid #AAA;
	-webkit-transition: none !important;
	   -moz-transition: none !important;
			transition: none !important;
}
table.chars.layout-flow tr:hover td.char-entity {top:-1px;left:100%;width:120px;height:60px;padding:0;border-width:1px 1px 0 0;}
table.chars.layout-flow tr:hover td.char-code {top:60px;left:100%;width:120px;height:60px;padding:0;border-width:0px 1px 0 0;}
table.chars.layout-flow tr:hover td.char-descr {top:100%;left:-1px;width:199px;height:80px;padding:20px;border-width:0px 1px 1px 1px;}

