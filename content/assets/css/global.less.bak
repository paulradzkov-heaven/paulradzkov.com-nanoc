	-webkit-transition:all 0.5s, font-size 0, line-height 0;
	   -moz-transition:all 0.5s, font-size 0, line-height 0;
			transition:all 0.5s, font-size 0, line-height 0;
}
.reset-animation {
	-webkit-transition: none !important;
	   -moz-transition: none !important;
			transition: none !important;
}


