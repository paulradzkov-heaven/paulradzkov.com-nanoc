html, body {margin:0;background:#FFF;color:#000;font:16px/1.4 PTSans,Verdana,Tahoma,Arial,Helvetica,"Bitstream Vera Sans",sans-serif;}
h1,h2,h3 {font-family:PTSans;font-weight:bold;line-height:1.2em;-webkit-font-smoothing:subpixel-antialiased;text-shadow:0 1px 0 #FFF;}
h1 a,h2 a,h3 a {text-decoration:none;} 
.allpage {
background: #FFF;
background:    -moz-radial-gradient(top left, ellipse cover,  rgba(10,108,135,0.1) 0%,rgba(255,255,255,0) 100%),    -moz-radial-gradient(top, ellipse cover,  rgba(146,204,165,0.1) 0%,rgba(255,255,255,0) 100%),     -moz-radial-gradient(top right, ellipse cover,  rgba(201,240,177,0.2) 0%,rgba(255,255,255,0) 100%); /* FF3.6+ */
background: -webkit-radial-gradient(top left, ellipse cover,  rgba(10,108,135,0.1) 0%,rgba(255,255,255,0) 100%), -webkit-radial-gradient(top, ellipse cover,  rgba(146,204,165,0.1) 0%,rgba(255,255,255,0) 100%),  -webkit-radial-gradient(top right, ellipse cover,  rgba(201,240,177,0.2) 0%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */
background:      -o-radial-gradient(top left, ellipse cover,  rgba(10,108,135,0.1) 0%,rgba(255,255,255,0) 100%),      -o-radial-gradient(top, ellipse cover,  rgba(146,204,165,0.1) 0%,rgba(255,255,255,0) 100%),       -o-radial-gradient(top right, ellipse cover,  rgba(201,240,177,0.2) 0%,rgba(255,255,255,0) 100%); /* Opera 12+ */
background:     -ms-radial-gradient(top left, ellipse cover,  rgba(10,108,135,0.1) 0%,rgba(255,255,255,0) 100%),     -ms-radial-gradient(top, ellipse cover,  rgba(146,204,165,0.1) 0%,rgba(255,255,255,0) 100%),      -ms-radial-gradient(top right, ellipse cover,  rgba(201,240,177,0.2) 0%,rgba(255,255,255,0) 100%); /* IE10+ */
background:         radial-gradient(top left, ellipse cover,  rgba(10,108,135,0.1) 0%,rgba(255,255,255,0) 100%),         radial-gradient(top, ellipse cover,  rgba(146,204,165,0.1) 0%,rgba(255,255,255,0) 100%),          radial-gradient(top right, ellipse cover,  rgba(201,240,177,0.2) 0%,rgba(255,255,255,0) 100%); /* W3C */
background-attachment: fixed;

}

/*
Sticky footer.
Compability: All

Styleguide 1.1.1.
*/
html, body {height:100%;margin-top:0;margin-bottom:0;}
.allpage {position:relative; min-height:100%; height:auto !important; height:100%;}
.pushfooter {height:60px;clear:both;}
.footer {margin:-30px 0 0 50px; position:relative;}

/* Page layout */
.pagefield {padding:40px 40px 40px 260px;}
.siteheader {position:relative;background:#000;color:#FFF;line-height:30px;z-index:150;overflow: hidden;}

.siteheader:before {
	position:absolute;
	z-index:-1;
	content:"";
	top:50%;
	bottom:0;
	left:30px;
	right:30px;
	-webkit-box-shadow:0 0 5px rgba(0,0,0,0.7),0 0 5px rgba(0,0,0,0.5);
	   -moz-box-shadow:0 0 5px rgba(0,0,0,0.7),0 0 5px rgba(0,0,0,0.5);
			box-shadow:0 0 5px rgba(0,0,0,0.7),0 0 5px rgba(0,0,0,0.5);
	-moz-border-radius:100% / 10px;
		 border-radius:100% / 10px;
}


/* text max-width */
p {max-width:50em;}
p:before {
  content: "";
  width: 20em;
  display: block;
  overflow: hidden;
}
li, dd {max-width:47em;}

.onright {float:right;margin:0 0 2em 2em;}
.onleft {float:left;margin:0 2em 2em 0;}

img {max-width:100%;}
img.noscale {max-width:auto;}

@media screen and (max-width:800px) {
	.pagefield {padding:40px 40px 40px 40px;	}
	html, body  {font-size:14px;}
}
@media screen and (max-width:550px) {
	.pagefield {padding:20px 20px 20px 20px;	}
	html, body  {font-size:13px;}
}
@media screen and (max-width:480px) {
	.pagefield {padding:10px 10px 10px 10px;	}
	html, body  {font-size:13px;}
}


/*
Footer styles
*/
.footer p {margin:0;font-size:0.8em;font-size:0.8rem;}
.copyright {float:left;width:210px;}
.thanx {float:left;}

/* header theme */
.siteheader a {color:#DDD !important;}
.siteheader a:hover {color:#FFF !important;}
.logo {text-align:center;width:260px;float:left;cursor:default;
	-webkit-transition: 0.5s;
	   -moz-transition: 0.5s;
			transition: 0.5s;
}
.logo:hover {text-shadow:0 0 6px #FFF;
	-webkit-transition: 0.5s;
	   -moz-transition: 0.5s;
			transition: 0.5s;
}
.logo:before {content:" < ";color:#666;font-family:Verdana;}
.logo:after {content:" />";color:#666;font-family:Verdana;}
.logo a {text-decoration:none;}
.nav {float:left;margin:0 0 0 -15px;}
@media screen and (max-width:800px) {
	.logo {width:210px;}
	.nav {float:left;margin:0 0 0 25px;}
}
.nav ul {display:inline;margin:0;padding:0;}
.nav ul li {display:inline;margin:0;padding:0;}
.nav ul li a,.nav ul li span {margin:0;padding:0 15px;}

.excerpt {margin-bottom:4em;}
.excerpt ul.tags {list-style-type:none;padding:0;}
.excerpt ul.tags li {padding:0 3px;display:inline;}
.article .meta {float:left;margin-left:-190px;}
.article ul.tags {list-style-type:none;padding:0;margin:0;}
.article ul.tags li {padding:0;}
.meta {font-size:0.8em;font-size:0.8rem;line-height:19px;line-height:1.2rem;}
@media screen and (max-width:800px) {
	.article .meta {float:none;margin-left:0px;}
	.article ul.tags li {padding:0 3px;display:inline;}
}


.about-me {margin-left:-190px;}
.about-me .userpic {width:120px;height:120px;float:left;margin-right:20px;}
.about-me .userpic .photo {width:120px;height:120px;position:relative;z-index:2;}
.helloworld {margin-left:190px;min-height:200px;line-height:1.75em;}
.helloworld p:first-child {margin-top:0;}
@media screen and (max-width:800px) {
	.about-me {margin-left:0px;}
	.helloworld {margin-left:0px;}
}
@media screen and (max-width:480px) {
	.about-me .userpic {width:60px;height:60px;margin-right:10px;}
	.about-me .userpic .photo {width:60px;height:60px;}
}


/* Shared styles */

.drop-shadow {
	position:relative;
	z-index:1;
}

.drop-shadow:before {
	content:"";
	position:absolute; 
	z-index:0;
}


/* Lifted corners */

.lifted {
	-moz-border-radius:4px; 
		 border-radius:4px;
}

.lifted:before
 { 
	bottom:5px;
	left:3px;
	width:50%;
	height:20%;
	max-width:60px;
	-webkit-box-shadow:0 5px 3px rgba(0, 0, 0, 0.5);   
	   -moz-box-shadow:0 5px 3px rgba(0, 0, 0, 0.5);
			box-shadow:0 5px 3px rgba(0, 0, 0, 0.5);
	-webkit-transform:rotate(-3deg);    
	   -moz-transform:rotate(-3deg);   
		-ms-transform:rotate(-3deg);   
		 -o-transform:rotate(-3deg);
			transform:rotate(-3deg);
}

.pushed:after {
	content:"";
	position:absolute; 
	z-index:0;
	bottom:-1px;
	right:-1px;
	width:90%;
	height:90%;
	background: -moz-linear-gradient(-45deg,  rgba(255,255,255,0) 50%, rgba(255,255,255,0.5) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right bottom, color-stop(50%,rgba(255,255,255,0)), color-stop(100%,rgba(255,255,255,0.5))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(-45deg,  rgba(255,255,255,0) 50%,rgba(255,255,255,0.5) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(-45deg,  rgba(255,255,255,0) 50%,rgba(255,255,255,0.5) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(-45deg,  rgba(255,255,255,0) 50%,rgba(255,255,255,0.5) 100%); /* IE10+ */
	background: linear-gradient(-45deg,  rgba(255,255,255,0) 50%,rgba(255,255,255,0.5) 100%); /* W3C */
}

.social a {white-space:nowrap;}

.featured {margin-bottom:8em;}
.blogroll {margin-bottom:8em;}

/* Tag cloud */
.tag-cloud {list-style-type:none;}
@media screen and (min-width:1280px) {
	aside.tag-cloud {position:absolute;left:72em;right:4em;top:78px;}
}

/* Archives */
.postlist {list-style:none;padding:0;}
.postlist > li {position:relative;display:block;padding:0;list-style:none;}
.timeline-mark {float:left;margin:0 -200px 0.67em;}
@media screen and (max-width:800px) {
	.timeline-mark {float:none;margin:0.67em 0;}
}
/* Other Articles */
#other-articles {
  margin-top: 4em; }
  #other-articles ul {
    padding: 0;
    list-style-type: none; }
  #other-articles h3 {
    margin: 0 0 0.5em; }
  #other-articles .newer {
    text-align: right;
    float: right; }
  #other-articles .older {
    float: left; }
#other-articles ul a {text-decoration:none;}
#other-articles ul a span {text-decoration:underline;}
.nav-label-newer, .nav-label-older {font-size:0.8em;font-size:0.8rem;}

/*=====================================
============== Typography =============
=======================================*/


/*
Links
Compability: All

Styleguide 1.1.1.
*/
a {color:#0069ff;}
a:link {color:#0069ff;}
a:visited {color:#003d94;}
a:hover {color:#00a1ff;}
a:focus {color:#00a1ff;}
a:active {color:#d50003;}

.dashed(@color: #0069ff) {
    background: -moz-linear-gradient(left, @color, @color 60%, transparent 60%);
    background: -ms-linear-gradient(left, @color, @color 60%, transparent 60%);
    background: -o-linear-gradient(left, @color, @color 60%, transparent 60%);
    background: -webkit-gradient(linear, 0 0, 100% 0, from(@color), color-stop(0.6, @color), color-stop(0.6, transparent));
    background: -webkit-linear-gradient(left, @color, @color 60%, transparent 60%);
    background: linear-gradient(left, @color, @color 60%, transparent 60%);
    background-size: 10px 1px;
    background-repeat: repeat-x;
    background-position: 0 100%;
}

.dotted(@color: #0069ff) {
    background: -moz-linear-gradient(left, @color, @color 50%, transparent 50%);
    background: -ms-linear-gradient(left, @color, @color 50%, transparent 50%);
    background: -o-linear-gradient(left, @color, @color 50%, transparent 50%);
    background: -webkit-gradient(linear, 0 0, 100% 0, from(@color), color-stop(0.5, @color), color-stop(0.5, transparent));
    background: -webkit-linear-gradient(left, @color, @color 50%, transparent 50%);
    background: linear-gradient(left, @color, @color 50%, transparent 50%);
    background-size: 2px 1px;
    background-repeat: repeat-x;
    background-position: 0 100%;
}
/*
Definitions.
Compability: all

Styleguide 1.1.3.
*/
dt {font-weight:bold;float:left;margin-right:0.2em;}

/*
Images decoration.
Compability: A-grade browsers

Styleguide 1.1.2.
*/
figure {margin:2em 0;margin:2rem 0;}
figure img {
    width: inherit;
    max-width:100%;
    height:auto;
    -ms-interpolation-mode: bicubic;
	-weblit-box-shadow:5px 5px 25px rgba(0,0,0,0.4),3px 3px 5px rgba(0,0,0,0.3);
	   -moz-box-shadow:5px 5px 25px rgba(0,0,0,0.4),3px 3px 5px rgba(0,0,0,0.3);
			box-shadow:5px 5px 25px rgba(0,0,0,0.4),3px 3px 5px rgba(0,0,0,0.3);
}
figure figcaption {margin:1em 0;margin:1rem 0;max-width:25em;font-size:0.9em;font-size:0.9rem;}

.left-image {
    float:left;
    margin: 0.2em 2em 1.5em 0;
}


.textlist {
    margin: 0;
    padding: 0;
    max-width:50em;
}
.textlist > li {
    margin: 0 0 .5em;
    padding-left: 2em;
    list-style-position: inside;
    text-indent:-1em;
    overflow: hidden;
}

.generatedcontent ul.textlist {
    list-style-type: none;
}
.generatedcontent ul.textlist > li {
    text-indent:0;
}
.generatedcontent ul.textlist > li:before {
    content: "\2014";
    display: inline-block;
    width: 1em;
    padding-right: 0.5em;
    margin-left: -1.5em;
}


/*
Force justification of text.
Compability: IE8+, A-grade browsers

.disabled          - Dims the button to indicate it cannot be used.

Styleguide 2.1.1.
*/
.force-justify {
	text-align:justify;
	text-align-last:justify; /* IE 7-8 specific */
}
.force-justify > .fj-item {
	display:inline;
	text-align:left;
	text-align-last:left; /* IE7 */
	white-space:pre-wrap; /* for IE8 only */
}
.force-justify .fj-item + .fj-item:before {
    content:"\0020";
	display:inline;
}

.force-justify:last-child:after {
    content:"\0020";
	display:inline-block;
	width:100%;
}

